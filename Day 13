//划分字母区间
vector<int> partitionLabels(string s)
{
    vector<int> ans;
    unordered_map<char, int> last;
    int size = s.size();
    int l = 0, r = 0;
    for (int i = 0; i < size; i++)
    {
        last[s[i]] = i;
    }
    for (int i = 0; i < size; i++)
    {
        r = max(r, last[s[i]]);
        if (r == i)
        {
            ans.push_back(r - l + 1);
            l = r + 1;
        }
    }
    return ans;
}
